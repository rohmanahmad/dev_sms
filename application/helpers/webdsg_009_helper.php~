<?php
function get_base_url(){
 return base_url('assets/websdg_009/');
}
function get_image($filename){
return base_url('assets/websdg_009/images/'.$filename);
}
function get_css($filename){
return base_url('assets/websdg_009/css/'.$filename);
}
function get_js($filename){
return base_url('assets/websdg_009/js/'.$filename);
}
function get_site_url($pagename=""){
return site_url ('webdsg_009/'.$pagename);

}


?>