<?php
function get_base_url(){
 return base_url('assets/websdg_011/');
}
function get_image($filename){
return base_url('assets/websdg_011/images/'.$filename);
}
function get_css($filename){
return base_url('assets/websdg_011/css/'.$filename);
}
function get_js($filename){
return base_url('assets/websdg_011/js/'.$filename);
}
function get_site_url($pagename=""){
return site_url ('webdsg_011/'.$pagename);

}


?>