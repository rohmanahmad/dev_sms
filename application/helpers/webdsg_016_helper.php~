<?php
function get_base_url(){
 return base_url('assets/websdg_016/');
}
function get_image($filename){
return base_url('assets/websdg_016/images/'.$filename);
}

function get_css($filename){
return base_url('assets/websdg_016/css/'.$filename);
}

function get_site_url($pagename=""){
return site_url ('webdsg_016/'.$pagename);

}


?>